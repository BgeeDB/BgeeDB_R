
citHeader("To cite reports in publications, please use:")



citEntry(entry = "manual",
    title = "BgeeDB: an R package for annotation and gene expression data retrieval from Bgee database",
    author = personList( as.person("Andrea Komljenovic"), as.person("Julien Roux")),
    note = "version 0.0.9",
    year = "2016",
    url = "http://github.com/wirawara/BgeeDB",
    textVersion  = paste("Komljenovic A, Roux J (2016).",
        "BgeeDB: an R package for annotation and gene expression data retrieval from Bgee database.",
        "version 0.0.9")
)

citEntry(entry="article",
         title = "Bgee: Integrating and Comparing Heterogeneous Transcriptome Data Among Species",
         author = personList( as.person("Bastian F"), as.person("Parmentier G"), as.person("Roux J")),
         year = "2008",
         journal = "Data Integration Life Sci",
         issue = "5109",
         pages= "124-31",
         textVersion =
           paste("Bastian F, Parmentier G, Roux J, et al. (2008):",
                 "Bgee: Integrating and Comparing Heterogeneous Transcriptome Data Among Species.",
                 "Data Integration Life Sci", "5109", "124-31" ) )
